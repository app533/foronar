pipeline {
  agent any
  stages {
    stage('Checkout') {
      git 'https://github.com/app533/foronar.git'
    }
    stage('SonarQube Analysis') {
      steps {
        withSonarQubeEnv('SonarQube') {
         sh 'sonar-scanner -D"sonar.projectKey=java" -D"sonar.sources=." -D"sonar.ho>
        }
      }
    }
  }
}
