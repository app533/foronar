pipeline {
  agent any
  stages {
    stage('Checkout') {
      steps {
        checkout scm
      }
    }
    stage('SonarQube Analysis') {
      steps {
        withSonarQubeEnv('SonarQube') {
         sh "${scannerHome}/bin/sonar-scanner\
         -D sonar.login=admin \
         -D sonar.password=admin \
         -D sonar.projectKey=java-jenkins-sonar12 \
         -D soonar.host.url=http://3.230.119.200:9000/"
        }
      }
    }
  }
}
